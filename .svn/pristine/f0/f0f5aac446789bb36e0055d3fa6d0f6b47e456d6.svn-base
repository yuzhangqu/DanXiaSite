body {
    margin: 0;
    padding: 0;
    font-family: microsoft yahei,Arial;
}

.indexmain {
    background-color: #c29b68;
    width: 1096px;
   
    margin: 0 auto;
    padding-top: 20px;
}

.main {
    margin: 0 auto;
    width: 1056px;
    
    border: 3px solid #F8F3DC;
}

.title {
    margin: 3px;
    width: 1050px;
}

.index {
    height: 40px;
}

    .index .head {
        margin-left: 5px;
        margin-top: 2px;
        border: 1px solid #EDDFB3;
        float: left;
        line-height: 30px;
        width: 288px;
        color: #840100;
        font-size: 22px;
        font-weight: bold;
        text-align: center;
    }

    .index .path {
        float: left;
        color: #413F43;
        font-size: 16px;
        width: 325px;
        margin-left: 20px;
        margin-top: 2px;
        line-height: 30px;
        background-color: #DAB27F;
        padding-left: 20px;
        border-top: 1px solid #7E664D;
        border-left: 1px solid #7E664D;
        border-bottom: 1px solid #EDDFB3;
        border-right: 1px solid #EDDFB3;
    }

.button01 {
    width: 170px;
    height: 35px;
    margin-left: 20px;
    background-image: url("img/button1.jpg");
    border: 0;
    color: #840100;
    font-weight: 900;
    font-size: 20px;
}

.nav {
    height: 48px;
    padding: 5px;
}


.button02 {
    width: 170px;
    height: 45px;
    background-image: url("img/button2.jpg");
    border: 0;
    color: white;
    font-weight: bold;
    font-size: 20px;
    font-family: microsoft yahei,Arial;
}


.button03 {
    width: 60px;
    height: 20px;
    background-color: #B30303;
    border: 0;
    color: white;
    font-weight: bold;
    font-size: 12px;
    font-family: microsoft yahei,Arial;
}

.main_left {
    width: 175px;
    padding-left: 5px;
    float: left;
}

.login {
    margin-top: 5px;
    margin-bottom: 5px;
    padding-left: 5px;
    height: 130px;
    width: 98%;
    background-color: #DAB27F;
    color: #840100;
    font-size: 12px;
    line-height: 30px;
}

.main_center {
    width: 694px;
    float: left;
}

.mainmodule {
    width: 98%;
    height: 300px;
    border: 4px solid #F8F3DC;
    margin-bottom: 10px;
    padding: 3px;
}

.link1 {
    color: #413F43;
    padding-left: 20px;
    margin-top: 5px;
    font-size: 16px;
    line-height: 30px;
}


.link2 {
    color: #413F43;
    padding-left: 10px;
    margin-top: 5px;
    font-size: 14px;
    line-height: 20px;
}


.link3 {
    color: #413F43;
    padding-left: 10px;
    margin-top: 5px;
    font-size: 14px;
    line-height: 23px;
}

.pic {
    margin-top: 5px;
    width: 170px;
    height: 150px;
}

.picview {
	width: 30%;
	height: 45%;
}


.main_right {
    padding-left: 5px;
    width: 175px;
    float: left;
}

.search {
    margin-top: 5px;
    margin-bottom: 5px;
    padding-left: 5px;
    height: 100px;
    width: 98%;
    background-color: #DAB27F;
    color: #840100;
    font-size: 12px;
    line-height: 30px;
}

.links1 {
    padding-left: 10px;
    float: left;
}

.links2 {
    float: left;
    color: #6F5F50;
    font-size: 14px;
    line-height: 25px;
}

    .links2 span {
        padding-left: 20px;
    }

.footer {
    margin-top: 10px;
    border-top: 4px solid #F8F3DC;
    height: 55px;
    background-color: #E0CC99;
    color: #2B3123;
    font-size: 16px;
    font-weight: bold;
    text-align: center;
    padding-top: 5px;
}
.content_pos{
	margin-left: 5px;
}
.content_long{
	height:auto;
}
.pic_center{
	text-align: center;
}

.pic_comunication{
	margin-top:5px;
	width:980px;
	height:700px;
	
	}
.newsDisplay{
	height:180px;
	margin-left:10px;
}

.toc{
	border: double 3px black;
	padding: 7px;
	font-size: 95%;
	width: 350px;
}

.toc ul{
	list-style: none;
	margin: 10px;
	padding: 0px;
}

.toc a{
	text-decoration: none;
	color: #0645ad;
	background: none;
}

.toctitle{
	text-align: center;
}

.toclevel-1{
	text-indent: 0em;
}

.toclevel-2{
	text-indent: 1em;
}

.toclevel-3{
	text-indent: 2em;
}

.toclevel-4{
	text-indent: 3em;
}