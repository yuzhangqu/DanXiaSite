				<value>com/dx/bean/TbGeologyscene.hbm.xml</value>
				<value>com/dx/bean/TbCulturescene.hbm.xml</value>
				<value>com/dx/bean/TbAuditinfo.hbm.xml</value>
				<value>com/dx/bean/TbCountrylevel.hbm.xml</value>
				<value>com/dx/bean/TbWorldlevel.hbm.xml</value>
				<value>com/dx/bean/TbLakelevel.hbm.xml</value>
				<value>com/dx/bean/TbUserinfo.hbm.xml</value>
				<value>com/dx/bean/TbNews.hbm.xml</value>
			</list>
		</property>
		<property name="hibernateProperties">
			<props>
				<prop key="hibernate.dialect">
					org.hibernate.dialect.MySQLDialect
				</prop>
			</props>
		</property>
	</bean>
